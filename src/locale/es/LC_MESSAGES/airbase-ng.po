#: airbase-ng.c:376
#, c-format
msgid "  Original work: Martin Beck\n"
msgstr "  Trabajo original: Martin Beck\n"

#: airbase-ng.c:379
#, c-format
msgid "  usage: airbase-ng <options> <replay interface>\n"
msgstr "  uso: airbase-ng <opciones> <interfaz>\n"

#: airbase-ng.c:381
#, c-format
msgid "  Options:\n"
msgstr "  Opciones:\n"

#: airbase-ng.c:383
#, c-format
msgid "      -a bssid         : set Access Point MAC address\n"
msgstr ""
"      -a bssid         : establece la dirección MAC del punto de acceso\n"

#: airbase-ng.c:384
#, c-format
msgid "      -i iface         : capture packets from this interface\n"
msgstr "      -i interfaz      : capturar paquetes desde esta interfaz\n"

#: airbase-ng.c:386
#, c-format
msgid "      -w WEP key       : use this WEP key to en-/decrypt packets\n"
msgstr ""
"      -w clave WEP     : usar esta clave WEP para cifrar/descifrar paquetes\n"

#: airbase-ng.c:389
#, c-format
msgid "      -h MAC           : source mac for MITM mode\n"
msgstr "      -h MAC           : MAC origen para modo MITM\n"

#: airbase-ng.c:390
#, c-format
msgid ""
"      -f disallow      : disallow specified client MACs (default: allow)\n"
msgstr ""
"      -f disallow      : rechazar las MAC de los clientes especificados (por "
"defecto las permite todas)\n"

#: airbase-ng.c:391
#, c-format
msgid ""
"      -W 0|1           : [don't] set WEP flag in beacons 0|1 (default: "
"auto)\n"
msgstr ""
"      -W 0|1           : [no] establecer el indicador WEP en los paquetes "
"baliza a 0|1 (por defecto: automático)\n"

#: airbase-ng.c:392
#, c-format
msgid "      -q               : quiet (do not print statistics)\n"
msgstr "      -q               : silencioso (no mostrar estadísticas)\n"

#: airbase-ng.c:393
#, c-format
msgid "      -v               : verbose (print more messages)\n"
msgstr "      -v               : verbosidad (mostrar más mensajes)\n"

#: airbase-ng.c:395
#, c-format
msgid "      -A               : Ad-Hoc Mode (allows other clients to peer)\n"
msgstr ""
"      -A               : Modo Ad-Hoc (permite que otros clientes se "
"conecten)\n"

#: airbase-ng.c:396
#, c-format
msgid "      -Y in|out|both   : external packet processing\n"
msgstr ""
"      -Y in|out|both   : procesamiento de paquetes externos (in = entrantes, "
"out = salientes, both = ambos)\n"

#: airbase-ng.c:397
#, c-format
msgid "      -c channel       : sets the channel the AP is running on\n"
msgstr "      -c canal         : establece el canal donde el AP se encuentra\n"

#: airbase-ng.c:398
#, c-format
msgid "      -X               : hidden ESSID\n"
msgstr "      -X               : ESSID oculto\n"

#: airbase-ng.c:399
#, c-format
msgid ""
"      -s               : force shared key authentication (default: auto)\n"
msgstr ""
"      -s               : forzar autenticación de clave compartida (por "
"defecto: automático)\n"

#: airbase-ng.c:400
#, c-format
msgid ""
"      -S               : set shared key challenge length (default: 128)\n"
msgstr ""
"      -S               : establecer longitud de clave compartida (por "
"defecto: 128)\n"

#: airbase-ng.c:401
#, c-format
msgid ""
"      -L               : Caffe-Latte WEP attack (use if driver can't send "
"frags)\n"
msgstr ""
"      -L               : ataque 'Caffe-Latte' para WEP (usar si el "
"controlador no puede enviar fragmentos)\n"

#: airbase-ng.c:402
#, c-format
msgid "      -N               : cfrag WEP attack (recommended)\n"
msgstr ""
"      -N               : ataque de fragmentación para WEP (recomendado)\n"

#: airbase-ng.c:403
#, c-format
msgid "      -x nbpps         : number of packets per second (default: 100)\n"
msgstr ""
"      -x npps          : número de paquetes por segundo (por defecto: 100)\n"

#: airbase-ng.c:404
#, c-format
msgid "      -y               : disables responses to broadcast probes\n"
msgstr "      -y               : desactiva respuesta a paquetes broadcast\n"

#: airbase-ng.c:405
#, c-format
msgid ""
"      -0               : set all WPA,WEP,open tags. can't be used with -z & -"
"Z\n"
msgstr ""
"      -0               : establece todos los tags a WPA ,WEP o abierta. No "
"puede ser usado con -z o -Z\n"

#: airbase-ng.c:406
#, c-format
msgid ""
"      -z type          : sets WPA1 tags. 1=WEP40 2=TKIP 3=WRAP 4=CCMP "
"5=WEP104\n"
msgstr ""
"      -z tipo          : establece los tags para WPA1. 1=WEP40 2=TKIP 3=WRAP "
"4=CCMP 5=WEP104\n"

#: airbase-ng.c:407
#, c-format
msgid "      -Z type          : same as -z, but for WPA2\n"
msgstr "      -Z tipo          : lo mismo que -z, pero para WPA2\n"

#: airbase-ng.c:408
#, c-format
msgid "      -V type          : fake EAPOL 1=MD5 2=SHA1 3=auto\n"
msgstr "      -V tipo          : EAPOL falso 1=MD5 2=SHA1 3=automático\n"

#: airbase-ng.c:409
#, c-format
msgid ""
"      -F prefix        : write all sent and received frames into pcap file\n"
msgstr ""
"      -F prefijo       : escribir todos los paquetes enviados y recibidos en "
"un archivo pcap\n"

#: airbase-ng.c:410
#, c-format
msgid ""
"      -P               : respond to all probes, even when specifying ESSIDs\n"
msgstr ""
"      -P               : responder a todos los paquetes de sondeo, aún "
"habiendo especificado ESSIDs\n"

#: airbase-ng.c:411
#, c-format
msgid "      -I interval      : sets the beacon interval value in ms\n"
msgstr ""
"      -I intervalo     : establece el intervalo de balizas en milisegundos\n"

#: airbase-ng.c:412
#, c-format
msgid ""
"      -C seconds       : enables beaconing of probed ESSID values (requires -"
"P)\n"
msgstr ""
"      -C seconds       : activa el embalizamiento de ESSIDs sondeados "
"(requiere -P)\n"

#: airbase-ng.c:414
#, c-format
msgid "  Filter options:\n"
msgstr "  Opciones de filtrado:\n"

#: airbase-ng.c:415
#, c-format
msgid "      --bssid MAC      : BSSID to filter/use\n"
msgstr "      --bssid MAC      : BSSID a filtrar/usar\n"

#: airbase-ng.c:416
#, c-format
msgid "      --bssids file    : read a list of BSSIDs out of that file\n"
msgstr "      --bssids archivo : leer una lista de BSSIDs de ese archivo\n"

#: airbase-ng.c:417
#, c-format
msgid "      --client MAC     : MAC of client to filter\n"
msgstr "      --client MAC     : MAC del cliente a filtrar\n"

#: airbase-ng.c:418
#, c-format
msgid "      --clients file   : read a list of MACs out of that file\n"
msgstr "      --clients archivo: leer una lista de MACs de ese archivo\n"

#: airbase-ng.c:419
#, c-format
msgid "      --essid ESSID    : specify a single ESSID (default: default)\n"
msgstr ""
"      --essid ESSID    : especificar un solo ESSID (por defecto: default)\n"

#: airbase-ng.c:420
#, c-format
msgid "      --essids file    : read a list of ESSIDs out of that file\n"
msgstr "      --essids archivo : leer una lista de ESSIDs de ese archivo\n"

#: airbase-ng.c:422
#, c-format
msgid "      --help           : Displays this usage screen\n"
msgstr "      --help           : Muestra esta ventana de ayuda\n"

